    <Title>
        <h1>Logged out</h1>
    </Title>
    <ChildContent>
        <div>
            You have been logged out.
        </div>
    </ChildContent>
</AccountContent>

@code {
    [Parameter]
    public string Realm { get; set; } = default!;
}